{
  "builds": [
    {
      "src": "EventHorizon/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "EventHorizon/wsgi.py"
    }
  ],
  "env": {
    "USE_S3_FOR_STATIC": "True"
  }
}
